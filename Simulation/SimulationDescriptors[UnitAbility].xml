      <SimulationModifierDescriptor TargetProperty="MaximumCityDefensePoint"   Operation="Multiplication" Value=".97" Path="../ClassEmpire/ClassCity" ValueMustBePositive="true"/>	  
      <SimulationModifierDescriptor TargetProperty="MaximumCityDefensePoint"   Operation="Subtraction" Value="10" Path="../ClassEmpire/ClassCity" ValueMustBePositive="true"/>	  
    </SimulationModifierDescriptors>
  </SimulationDescriptor>

  <!-- Yggdra Demon's Bounty-->
